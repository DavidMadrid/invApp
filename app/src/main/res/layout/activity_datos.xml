<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.david.invapp.DatosActivity">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout3">

    </LinearLayout>

    <TextView
        android:layout_width="300px"
        android:layout_height="wrap_content"
        android:text="Centro"
        android:id="@+id/tvCentro"
        android:layout_marginTop="34dp"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:id="@+id/linearLayout4"></LinearLayout>

    <TextView android:text="Mostrar Centro" android:id="@+id/btnRunForm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvCentro"
        android:layout_alignBottom="@+id/tvCentro"
        android:layout_toRightOf="@+id/tvCentro"
        android:layout_toEndOf="@+id/tvCentro">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recuento"
        android:id="@+id/textView4"
        android:layout_below="@+id/tvCentro"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvMostrarRecuento"
        android:layout_alignTop="@+id/textView4"
        android:layout_toRightOf="@+id/tvCentro"
        android:text="MostrarRecuento" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha"
        android:id="@+id/tvFecha"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MostrarFecha"
        android:id="@+id/tvMostrarfecha"
        android:layout_alignTop="@+id/tvFecha"
        android:layout_toRightOf="@+id/tvCentro"
        android:layout_toEndOf="@+id/tvCentro" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Almacen"
        android:id="@+id/tvAlmacen"
        android:layout_below="@+id/tvFecha"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="126dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicial"
        android:id="@+id/tvAlmacenInicial"
        android:layout_below="@+id/tvAlmacen"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=".........."
        android:id="@+id/tvAlmacenMostrarI"
        android:layout_below="@+id/tvAlmacen"
        android:layout_toRightOf="@+id/tvAlmacenInicial"
        android:layout_toEndOf="@+id/tvAlmacenInicial" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Final"
        android:id="@+id/tvAlmacenFinal"
        android:layout_above="@+id/tvUbicacion"
        android:layout_alignLeft="@+id/tvUbicacionFinal"
        android:layout_alignStart="@+id/tvUbicacionFinal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="......."
        android:id="@+id/tvMostrarAlmacenF"
        android:layout_alignTop="@+id/tvAlmacenFinal"
        android:layout_toRightOf="@+id/tvMostrarRecuento"
        android:layout_toEndOf="@+id/tvMostrarRecuento" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ubicacion"
        android:id="@+id/tvUbicacion"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/tvAlmacenInicial"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicial"
        android:id="@+id/tvUbicacionInicial"
        android:layout_below="@+id/tvUbicacion"
        android:layout_toLeftOf="@+id/tvAlmacenMostrarI"
        android:layout_toStartOf="@+id/tvAlmacenMostrarI" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="............"
        android:id="@+id/tvUbicacionMostrarI"
        android:layout_below="@+id/tvUbicacion"
        android:layout_toRightOf="@+id/tvAlmacen"
        android:layout_toEndOf="@+id/tvAlmacen" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Final"
        android:id="@+id/tvUbicacionFinal"
        android:layout_alignTop="@+id/tvUbicacionMostrarI"
        android:layout_toLeftOf="@+id/tvMostrarAlmacenF"
        android:layout_toStartOf="@+id/tvMostrarAlmacenF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="........"
        android:id="@+id/tvMostrarUbicacionF"
        android:layout_alignTop="@+id/tvUbicacionFinal"
        android:layout_toRightOf="@+id/tvMostrarRecuento"
        android:layout_toEndOf="@+id/tvMostrarRecuento" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Articulo"
        android:id="@+id/tvArticulo"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/tvUbicacionInicial"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicial"
        android:id="@+id/tvArticuloInicial"
        android:layout_below="@+id/tvArticulo"
        android:layout_alignLeft="@+id/tvArticulo"
        android:layout_alignStart="@+id/tvArticulo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=".........."
        android:id="@+id/tvArticuloInicial"
        android:layout_below="@+id/tvArticulo"
        android:layout_alignLeft="@+id/tvUbicacionMostrarI"
        android:layout_alignStart="@+id/tvUbicacionMostrarI" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Final"
        android:id="@+id/tvArticuloFinal"
        android:layout_alignTop="@+id/tvArticuloInicial"
        android:layout_alignLeft="@+id/tvUbicacionFinal"
        android:layout_alignStart="@+id/tvUbicacionFinal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="......"
        android:id="@+id/tvMostrarArticuloF"
        android:layout_alignTop="@+id/tvArticuloFinal"
        android:layout_alignLeft="@+id/tvMostrarUbicacionF"
        android:layout_alignStart="@+id/tvMostrarUbicacionF" />

</RelativeLayout>
